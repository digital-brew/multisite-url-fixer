{
  "name": "digital-brew/multisite-url-fixer",
  "type": "wordpress-muplugin",
  "version": "1.0.1",
  "license": "MIT",
  "description":
    "Fixes WordPress issues with home and site URL on multisite when using Themosis",
  "homepage": "https://github.com/digital-brew/multisite-url-fixer/",
  "keywords": ["wordpress"],
  "autoload": {
    "psr-4": {
      "DigitalBrew\\MultisiteURLFixer\\": "src/"
    }
  },
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.1"
  },
  "scripts": {
    "test": ["phpcs --ignore=vendor --extensions=php --standard=PSR2 ."]
  }
}
